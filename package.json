{
  "name": "localsurvey",
  "homepage": "https://decrn.github.io/localsurvey",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^3.4.0",
    "antd": "^3.13.0",
    "craco-antd": "^1.10.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.13",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "jest": "^24.0.0",
    "react-scripts": "2.1.3",
    "react-test-renderer": "^16.7.0",
    "typescript": "3.2.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "jest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-config/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/test-config/jest-setup.js"
    ],
    "testMatch": [
      "**/*.spec.(ts|tsx|js)"
    ]
  },
  "browserslist": [
    ">2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
