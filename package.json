{
  "name": "localsurvey",
  "homepage": "https://decrn.github.io/localsurvey",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^3.4.0",
    "antd": "^3.13.0",
    "connected-react-router": "^6.2.2",
    "craco-antd": "^1.10.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.13",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-redux": "^7.0.1",
    "@types/react-router-dom": "^4.3.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1",
    "react-scripts": "2.1.3",
    "react-test-renderer": "^16.7.0",
    "redux-devtools-extension": "^2.13.7",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "3.2.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build && cp build/index.html build/404.html",
    "test": "jest",
    "tslint": "tslint -c tslint.json -p tsconfig.json './src/**/*.{ts,tsx}'",
    "tslint:fix": "tslint --fix -c tslint.json -p tsconfig.json './src/**/*.{ts,tsx}'",
    "prettier": "prettier --write '**/*.{js,ts,css,less,md,json,yml}'",
    "prettier:check": "prettier --list-different '**/*.{js,ts,tsx,less,md,json,yml}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/test-config/preprocessor.js"
    },
    "setupFiles": [
      "<rootDir>/test-config/jest-setup.js"
    ],
    "testMatch": [
      "**/*.spec.(ts|tsx|js)"
    ]
  },
  "lint-staged": {
    "concurrent": false,
    "linters": {
      "src/**/*.{ts,tsx}": [
        "tslint --fix -c tslint.json",
        "git add"
      ],
      "**/*.{js,ts,tsx,less,md,json,yml}": [
        "prettier --write",
        "git add"
      ]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
